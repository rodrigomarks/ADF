{
	"name": "json_blob_trusted",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lnkblobrodrigo",
			"type": "LinkedServiceReference"
		},
		"type": "AzureBlob",
		"structure": [
			{
				"name": "firstname",
				"type": "String"
			},
			{
				"name": "lastname",
				"type": "String"
			},
			{
				"name": "gender",
				"type": "String"
			},
			{
				"name": "location",
				"type": "String"
			},
			{
				"name": "subscription_type",
				"type": "String"
			}
		],
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "\n",
				"quoteChar": "\"",
				"nullValue": "\\N",
				"encodingName": null,
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": true
			},
			"fileName": {
				"value": "@contains('part','fileName')",
				"type": "Expression"
			},
			"folderPath": "trusted/json"
		}
	}
}