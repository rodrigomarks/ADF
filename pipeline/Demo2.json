{
	"name": "Demo2",
	"properties": {
		"activities": [
			{
				"name": "Local2Cloud",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "ML Batch Execution1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "FileSystemSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "FileShare",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlob",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "ML Batch Execution1",
				"type": "AzureMLBatchExecution",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"webServiceInputs": {
						"": {
							"filePath": "",
							"linkedServiceName": {
								"referenceName": "AzureBlobStorage1",
								"type": "LinkedServiceReference"
							}
						}
					},
					"webServiceOutputs": {
						"": {
							"filePath": "",
							"linkedServiceName": {
								"referenceName": "AzureBlobStorage2",
								"type": "LinkedServiceReference"
							}
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureML1",
					"type": "LinkedServiceReference"
				}
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}