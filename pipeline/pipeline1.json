{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "data ingestion",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"OriginAirportCode": "OriginAirportCode",
							"Month": "Month",
							"DayofMonth": "DayofMonth",
							"CRSDepHour": "CRSDepHour",
							"DayOfWeek": "DayOfWeek",
							"Carrier": "Carrier",
							"DestAirportCode": "DestAirportCode",
							"DepDel15": "DepDel15",
							"WindSpeed": "WindSpeed",
							"SeaLevelPressure": "SeaLevelPressure",
							"HourlyPrecip": "HourlyPrecip"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "flightdelay_blob_temp",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "flightdelay_blob_raw",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "flightdelay"
		}
	}
}