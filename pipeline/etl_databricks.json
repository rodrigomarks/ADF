{
	"name": "etl_databricks",
	"properties": {
		"activities": [
			{
				"name": "ETL",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "Blob_SQLDW",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"notebookPath": "/lab1/blobscala"
				},
				"linkedServiceName": {
					"referenceName": "lnketljson",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Blob_SQLDW",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"writeBatchSize": 10000,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": false
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "json_blob_trusted",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sqldw",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "etldatabricks"
		}
	}
}