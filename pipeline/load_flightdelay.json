{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "data ingestion",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink",
						"copyBehavior": "MergeFiles"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "flightdelay_blob_temp",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "flightdelay_blob_raw",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "flightdelay"
		}
	}
}